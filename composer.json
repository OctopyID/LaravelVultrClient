{
    "name": "octopyid/laravel-vultr",
    "description": "Vultr API Client for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Supian M",
            "email": "supianidz@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^v8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.9",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "Octopy\\Vultr\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Octopy\\Vultr\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Octopy\\Vultr\\ServiceProvider"
            ]
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
